#include <nxp/mcx/MCXC444VLH-pinctrl.h>

&pinctrl {
	pinmux_lpuart0: pinmux_lpuart0 {
		group0 {
			pinmux = <LPUART0_RX_PTA1>,
				<LPUART0_TX_PTA2>;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};
	pinmux_adc0: pinmux_adc0 {
		group0 {
			pinmux = <ADC0_SE0_PTE20>,
				<ADC0_SE3_PTE22>;
			drive-strength = "low";
			slew-rate = "slow";
		};
	};
};
