config MFD_AW9523B
	bool "Awinic AW9523B GPIO/LED controller device driver"
	default y
	depends on DT_HAS_AWINIC_AW9523B_ENABLED
	select I2C
	help
	  Enable the Awinic AW9523B GPIO/LED controller device driver

config MFD_AW9523B_INIT_PRIORITY
	int "nPM1300 MFD initialization priority"
	default 60
	depends on MFD_AW9523B
	help
	  Initialization priority for AW9523B.
