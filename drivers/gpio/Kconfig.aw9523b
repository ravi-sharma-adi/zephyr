config GPIO_AW9523B
	bool "AW9523B GPIO driver"
	default y
	depends on DT_HAS_AWINIC_AW9523B_GPIO_ENABLED
	select I2C
	select MFD
	help
	  Enable the AW9523B GPIO controller.

config GPIO_AW9523B_INIT_PRIORITY
	int "Init priority"
	default 80
	depends on GPIO_AW9523B
	help
	  AW9523B GPIO driver initialization priority.
