&i2c0 {
	eeprom0: eeprom@54 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x54>;
		size = <256>;
	};
	eeprom1: eeprom@56 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x56>;
		size = <256>;
	};
	eeprom2: eeprom@58 {
		compatible = "zephyr,i2c-target-eeprom";
		reg = <0x58>;
		size = <4096>;
		address-width = <16>;
		read-only;
	};
};
