/ {
	aliases {
		sdhc0 = &sdhc0;
	};
};

&sdhc0 {
	status = "okay";
	sdmmc {
		compatible = "zephyr,sdmmc-disk";
		disk-name = "SD";
		status = "okay";
		partition {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size_cells = <1>;
			slot1_partition: partition@0 {
				reg = <0x00000000 0x800000>;
			};
		};
	};
};
